<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 圆形 ImageView属性 -->
    <attr name="borderRadius" format="dimension" />
    <attr name="type">
        <enum name="circle" value="0" />
        <enum name="round" value="1" />
    </attr>

    <declare-styleable name="RoundImageViewByXfermode">
        <attr name="borderRadius" />
        <attr name="type" />
    </declare-styleable>

    <declare-styleable name="MySelectCount">
        <attr name="size" format="integer" />
        <attr name="icon_color" format="color" />
        <attr name="minValue" format="integer" />
        <attr name="canInput" format="boolean" />
    </declare-styleable>
</resources>